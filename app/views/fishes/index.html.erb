<h1>Fishes</h1>

<table class="table">
 <tr class='table-headers'>
   <td> Name </td>
   <td> Age </td>
 </tr>
  
 <% @fishes.each do |fish| %>
    <tr>
    <td>
      <%= link_to fish.name, edit_fish_path(fish) %>
    </td>
    <td>
      <%= fish.age %>
    </td>
    </tr>
  <% end %>

</table>
<br/>
<%= link_to 'Create New Fish', new_fish_path %>
